{
  "version": 2,
  "builds": [
    {
      "src": "api/cron.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/cron",
      "dest": "/api/cron.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/cron.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "crons": [
    {
      "path": "/cron",
      "schedule": "40 23 * * *"
    }
  ]
}